@model Vanessa.Models.Proyecto
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Create</title>
</head>
<body>
   

    <h1>Crear Proyecto</h1>

    <form asp-action="Create" method="post" enctype="multipart/form-data">
        <div class="form-group">
            <label for="Nombre">Nombre</label>
            <input type="text" class="form-control" id="Nombre" name="Nombre" required />
        </div>

        <div class="form-group">
            <label for="EquiposInvestigacion">Equipos de Investigación</label>
            <input type="text" class="form-control" id="EquiposInvestigacion" name="EquiposInvestigacion" required />
        </div>

        <div class="form-group">
            <label for="FechaInicio">Fecha de Inicio</label>
            <input type="date" class="form-control" id="FechaInicio" name="FechaInicio" required />
        </div>

        <div class="form-group">
            <label for="pdfFile">Archivo PDF</label>
            <input type="file" class="form-control" id="pdfFile" name="pdfFile" accept="application/pdf" />
        </div>

        <button type="submit" class="btn btn-primary">Guardar</button>
    </form>
</body>
</html>
